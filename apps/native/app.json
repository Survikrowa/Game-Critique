{
  "expo": {
    "name": "Game Critique",
    "slug": "native",
    "scheme": "acme",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "owner": "game-critique",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "splash": {
      "image": "./assets/splash-2.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gamecritique.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.gamecritique.mobile",
      "permissions": ["INTERNET"]
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "newArchEnabled": true
          },
          "ios": {
            "newArchEnabled": true
          }
        }
      ],
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent",
          "addGeneratedScheme": false
        }
      ],
      "expo-router",
      [
        "react-native-auth0",
        {
          "domain": "dev-3gebv0fjdsc0gf5g.us.auth0.com"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-font",
      "expo-secure-store",
      "expo-asset"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "721093c7-a08f-4258-aad8-eac3c10e49cb"
      }
    }
  }
}
