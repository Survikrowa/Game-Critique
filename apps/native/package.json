{
  "name": "game-critique",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "prebuild": "npx expo prebuild --clean",
    "dev": "NODE_ENV=production adb reverse tcp:3000 tcp:3000 && EXPO_USE_METRO_WORKSPACE_ROOT=1 expo run:android",
    "dev-local": "adb reverse tcp:3001 tcp:3001 && expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint .",
    "generate-graph": "dotenvx run --env-file=.env.local -- graphql-codegen",
    "generate-graph-prod": "graphql-codegen",
    "build-android": "NODE_ENV=production eas build --clear-cache --platform android --local",
    "build-ios": "NODE_ENV=production eas build --clear-cache --platform ios --local",
    "build-android-dev": "NODE_ENV=development eas build --profile development --platform android",
    "eas-build-post-install": "yarn generate-graph-prod"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@dev-plugins/apollo-client": "^0.3.1",
    "@dotenvx/dotenvx": "^1.41.0",
    "@expo/webpack-config": "^19.0.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typescript-react-apollo": "^4.1.0",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^3.3.4",
    "@react-native-community/slider": "5.0.1",
    "@react-navigation/drawer": "^7.5.0",
    "@tamagui/config": "1.79.6",
    "@tamagui/lucide-icons": "1.79.6",
    "@tamagui/toast": "1.79.6",
    "axios": "^1.6.7",
    "burnt": "^0.12.2",
    "expo": "^54.0.0",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-device": "~8.0.10",
    "expo-document-picker": "~14.0.8",
    "expo-font": "~14.0.11",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.22",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "graphql": "^16.8.1",
    "moti": "^0.30.0",
    "react": "19.1.0",
    "react-content-loader": "^7.0.2",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.49.3",
    "react-native": "0.81.5",
    "react-native-auth0": "^3.1.0",
    "react-native-floating-action": "^1.22.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-gifted-charts": "^1.4.61",
    "react-native-reanimated": "~4.1.1",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-svg-transformer": "^1.3.0",
    "react-native-web": "^0.21.0",
